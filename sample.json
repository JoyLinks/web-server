[
	{
		"port": 80,
		"servlets": [],
		"resources": [],
		"locations": [],
		"authenticates": [],
		"headers": [],
		"roster": [],
		"access": "access/access.log",
		"hosts": [
			{
				"names": [
					"www.joyzl.com"
				],
				"webdavs": [
					{
						"path": "/webdav",
						"content": "/var/www/webdav",
						"preflight": true
					}
				],
				"servlets": [],
				"locations": [
					{
						"path": "/webdav",
						"location": "/webdav/"
					}
				],
				"resources": [
					{
						"path": "*",
						"cache": "caches",
						"content": "/var/www/joyzl",
						"defaults": [
							"index.html"
						],
						"compresses": [],
						"caches": [],
						"error": "/var/www/joyzl/errors",
						"browse": false,
						"weak": true
					}
				],
				"authenticates": [
					{
						"path": "/limited",
						"type": "Basic",
						"realm": "protected resources",
						"algorithm": "MD5",
						"methods": []
					}
				],
				"headers": {
					"X-Frame-Options": "DENY",
					"Strict-Transport-Security": "max-age=31536000; includeSubDomains",
					"Content-Security-Policy": "frame-ancestors 'none';"
				},
				"roster": [
					{
						"deny": true,
						"host": "192.168.0.1"
					},
					{
						"deny": true,
						"host": "192.168.2.3"
					}
				],
				"access": "/var/www/joyzl.access/access.log"
			}
		]
	}
]