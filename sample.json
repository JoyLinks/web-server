[
	{
		"port": 80,
		"servlets": [],
		"resources": [],
		"authenticates": [],
		"roster": [],
		"access": "access/access.log",
		"hosts": [
			{
				"names": [
					"www.joyzl.com"
				],
				"servlets": [],
				"resources": [
					{
						"path": "*",
						"cache": "caches",
						"content": "/var/www/joyzl",
						"defaults": [
							"index.html"
						],
						"compresses": [],
						"caches": [],
						"error": "/var/www/joyzl/errors",
						"browse": false,
						"weak": true
					}
				],
				"authenticates": [
					{
						"path": "/limited",
						"type": "Basic",
						"realm": "protected resources",
						"algorithm": "MD5",
						"methods": [],
						"users": "/var/www/joyzl.user"
					}
				],
				"roster": [
					{
						"deny": true,
						"host": "192.168.0.1"
					},
					{
						"deny": true,
						"host": "192.168.2.3"
					}
				],
				"access": "/var/www/joyzl.access/access.log"
			}
		]
	}
]