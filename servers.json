[
	{
		"port": 80,
		"servlets": [
			"com.joyzl.webserver.servlets"
		],
		"webdavs": [
			{
				"path": "/webdav/*",
				"content": "D:\\GitHub\\HTTP-TEST\\webdav"
			}
		],
		"locations": [
			{
				"path": "/webdav",
				"location": "/webdav/"
			},
			{
				"path": "/joyzl",
