[
	// Servers
	{
		"port": 80,
		"type": "HTTP",
		"name": "DEFAULT 80",
		"access": "access\/default",
		"authentications": [
			{
				"path": "/manage/",
				"realm": "Server manage",
				"type": "None"
			}
		],
		"servlets": [
			{
				"path": "*",
				"type": "RESOURCE",
				"parameters": {
					"cache": "cache",
					"error": "www/error",
					"content": "www",
					"browsable": true
				},
				"headers": {
					"X-Frame-Options": "DENY"
				}
			},
			{
				"type": "ARCHIVE",
				"parameters": {
					"content": "archives",
					"expire": 90
				}
			},
			{
				"type": "SETTING",
				"headers": {},
				"parameters": {}
			},
			{
				"type": "VISITS",
				"headers": {},
				"parameters": {}
			},
			{
				"type": "ROSTER",
				"headers": {},
				"parameters": {}
			},
			{
				"type": "USER",
				"headers": {},
				"parameters": {}
			},
			{
				"type": "LOG",
				"headers": {},
				"parameters": {}
			},
			{
				"type": "IP",
				"headers": {},
				"parameters": {}
			},
			{
				"type": "EMOJI",
				"headers": {},
				"parameters": {}
			}
		]
	},
	{
		"port": 443,
		"type": "HTTPS",
		"name": "DEFAULT 443",
		"access": "access\/access.log"
	}
]